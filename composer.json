{
  "require": {
    "craftcms/cms": "3.7.27.2",
    "craftcms/redactor": "2.8.8",
    "ibericode/vat": "^2.0",
    "jamesedmonston/graphql-authentication": "^1.10",
    "studioespresso/craft-dumper": "2.0.0",
    "studioespresso/craft-mollie-payments": "2.2.0",
    "verbb/formie": "1.6.6",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
